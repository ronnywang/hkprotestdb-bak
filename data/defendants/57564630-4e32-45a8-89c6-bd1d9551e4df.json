[[{"id":"348b258c-bb15-4c0c-890c-24fc32d07a24","case_num":"DCCC315/2021","type":"original","subtype":null,"next_hearing_date":null,"close_date":null,"judge":null,"remark_zh":null,"remark_en":null,"references_zh":null,"references_en":null,"thisDefendant":[{"remark_zh":"於2021年3月17日起被還柙，至4月27日獲准保釋，但需遵守嚴苛保釋條件，包括每周三次到警署報到、交出旅遊證件及 BNO、遵守朝10晚 8宵禁等。","remark_en":"","references_zh":"","references_en":"","person":{"id":"57564630-4e32-45a8-89c6-bd1d9551e4df","initial_zh":"陳","initial_en":"","hide_name":true,"gender":"female","image_url":null,"case_person":[{"arrest_age":null,"occupation":{"type_zh":"學生","type_en":"學生"},"status":{"id":1,"type_zh":"保釋","type_en":"保釋"}},{"arrest_age":null,"occupation":{"type_zh":"學生","type_en":"學生"},"status":{"id":2,"type_zh":"還柙","type_en":"還柙"}}]},"total_sentence_zh":"","total_sentence_en":"","occupation":{"type_zh":"學生","type_en":"學生"},"status":{"id":1,"type_zh":"保釋","type_en":"保釋"},"charges":[{"id":"5b278bcd-2d54-4ce3-885d-30bc6e2dea38","content_zh":"於2019年11月11日與於或約於11月14日之間（首尾兩日包括在內），在香港串謀與其他不知名的人於香港理工大學參與暴動。","content_en":"","charge":{"id":62,"name_zh":"串謀參與暴動","name_en":""},"sentence_sp_zh":null,"sentence_sp_en":null,"sentence":null,"sentence_2":null,"sentence_3":null,"status":null,"penalty":null,"penalty_2":null,"penalty_3":null,"arrest_location_zh":"理工大學","arrest_location_en":null,"plead_guilty":"pending_answer","start_date":"2019-11-11","remark_zh":"","remark_en":""}]}],"judge_2":null,"judge_3":null,"incident":{"id":66,"date":"2019-11-17","name_zh":" 理大圍城．校內突圍（第一天）","name_en":"","desc_zh":"【11月16日至29日 — 理大圍城13天】\n\n為配合11.11「大三罷」，示威者以理大為據點留守，堵塞紅隧。至16日晚上，警方與示威者在漆咸道南交戰，並在晚上近9時半宣布將於理大展開進一步行動，呼籲所有人立即循Y座出口離開。\n\n惟當時多輛警車及防暴警察已在出口對開佈防，多名身穿醫生、護士、急救員反光衣的人被捕，遭圍封要求坐地，雙手被索帶反綁。\n\n激烈衝突延續至17日早上，隨著校內情況愈來愈嚴峻，被困者突圍失敗，多人受傷和被捕，有人倒地後遭警員用腳踩頭。","desc_en":"","location":null},"progress":{"id":1,"type_zh":"提堂","type_en":"提堂"},"child_cases":[],"parent_cases":[{"id":"911a1f9f-3e80-4a49-af2c-63bf15b71e04","relation_name":{"id":1,"name_zh":"轉介","name_en":"Referred"